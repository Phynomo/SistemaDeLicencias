@model SistemaLicencias.WebUI.Models.SolicitantesViewModel


<div class="modal" id="ModalDelete" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" style="margin:auto;">Eliminar</h5>
            </div>
            <form asp-action="Delete" id="formDelete">
                <div class="modal-body">
                    <div class="modal-body text-center">
                        <div class="row mb-3">
                            <div class="col">
                                <input asp-for="soli_Id" class="form-control" id="tili_IdD" hidden />
                                <h4 class="padding-top-30 mb-30 weight-500">¿Desea eliminar este registro?</h4>
                                <label name="lbl_DescripcionDeleteError" id="lbl_DescripcionDeleteError" class="control-label" hidden style="color: red;">Ocurrio un error con este delete</label>

                            </div>
                        </div>
                        <div class="row">
                            <div class="col">
                                <button type="button" class="btn btn-danger mx-1" onclick="EnviarModalDelete()">Eliminar</button>
                                <button type="button" class="btn btn-dark" data-dismiss="modal">Cerrar</button>
                            </div>
                        </div>

                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
